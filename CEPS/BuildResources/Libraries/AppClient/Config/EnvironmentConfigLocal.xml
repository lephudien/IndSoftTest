    <GroupToGroupConverters>
      <GroupToGroupConverter Name="Test" ProgId="Enif.Data.CUnitConverterTest,Enif">
        <GroupName>volume</GroupName>
        <GroupName>energy</GroupName>
        <GroupName>CZK</GroupName>
